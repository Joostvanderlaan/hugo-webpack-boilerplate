    "node-sass": "4.5.x",
    "postcss-at2x": "4.0.x",
    "postcss-loader": "2.0.x",
    "postcss-sprites": "4.2.x",
    "sass-loader": "6.0.x",
    "sharp-image-loader": "0.1.0-alpha.1",
    "spawn-loader": "3.0.x",
