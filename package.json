    "prepublish": "babel src/ -d $npm_package_main",
    "pretest": "semistandard | snazzy"
  },
  "dependencies": {
    "autoprefixer": "7.1.x",
    "babel-core": "6.25.x",
    "babel-loader": "7.1.x",
