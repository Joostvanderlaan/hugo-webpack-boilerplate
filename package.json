{
  "name"        : "hugo-webpack-boilerplate",
  "version"     : "1.0.0",
  "description" : "Hugo and Webpack boilerplate.",
  "keywords"    : [ "boilerplate", "hugo", "hugo-boilerplate", "hugo-webpack", "webpack", "webpack-boilerplate" ],
  "author"      : "Mark van Seventer <mark@vseventer.com> (https://www.vseventer.com)",
  "homepage"    : "https://github.com/vseventer/hugo-webpack-boilerplate",
  "bugs"        : "https://github.com/vseventer/hugo-webpack-boilerplate/issues",
  "license"     : "MIT",
  "repository"  : { "type": "git", "url": "vseventer/hugo-webpack-boilerplate" },
  "main"        : "build/",
